{{!<ul>}}
    {{#each (sort-by 'Name' items) as |item|}}
        <li data-test-datacenter-picker="{{item.Name}}" class={{if (is-href 'dc.services' item.Name) 'is-active'}}>
            <a onclick={{action onchange}} href={{href-to 'dc.services' item.Name}}>{{item.Name}}</a>
        </li>
    {{/each}}
{{!</ul>}}